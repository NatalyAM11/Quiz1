package view;
import processing.core.PApplet;

public class Figura {
	
	protected int posX;
	protected int posY;
	protected int tam;
	protected int vel;
	protected int tipo;
	PApplet app;
	
 public Figura (int posX, int posY, int tam,String tipo, int vel, PApplet app) {
	 this.posX=posX;
	 this.posY=posY;
	 this.tam=tam;
	 this.vel=vel;
	 this.app=app;
 }
 
 public void pintar() {
	 
 }
 public void mover() {
	 
 }
}
